<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="464.0" prefWidth="405.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.address.controller.PersonDataController">
   <children>
      <TableView layoutX="-2.0" layoutY="224.0" prefHeight="239.0" prefWidth="407.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="50.0" text="Name" />
          <TableColumn fx:id="surnameColumn" prefWidth="241.0" text="Surname" />
            <TableColumn fx:id="phoneColumn" prefWidth="50.0" text="Phone" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label layoutX="56.0" layoutY="20.0" text="Name" />
      <Label fx:id="surnameLabel" layoutX="44.0" layoutY="72.0" text="Surname" />
      <Label layoutX="54.0" layoutY="121.0" text="Phone" />
      <TextField fx:id="surnameText" layoutX="115.0" layoutY="66.0" />
      <TextField fx:id="phoneText" layoutX="115.0" layoutY="115.0" />
      <ButtonBar layoutX="54.0" layoutY="176.0" prefHeight="40.0" prefWidth="293.0">
        <buttons>
            <Button mnemonicParsing="false" text="Update" />
            <Button mnemonicParsing="false" text="Delete" />
          <Button fx:id="addButton" mnemonicParsing="false" onAction="#addPerson" text="Add" />
        </buttons>
      </ButtonBar>
      <TextField fx:id="nameText" layoutX="115.0" layoutY="14.0" />
   </children>
</AnchorPane>
